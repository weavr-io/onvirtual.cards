{
    "name": "onvirtual-cards",
    "version": "1.0.0",
    "description": "onvirtual.cards for weavr",
    "author": "Threls Ltd.",
    "private": true,
    "scripts": {
        "dev": "nuxt dev",
        "dev:host": "nuxt dev --host",
        "build": "nuxt build",
        "start": "nuxt start",
        "lint": "eslint --ext .ts,.jsx,.js,.html,.vue . && stylelint \"**/*.{css,scss}\"",
        "lint:fix": "eslint --fix --ext .ts,.jsx,.js,.html,.vue . && stylelint \"**/*.{css,scss}\" --fix",
        "lint:ci": "eslint --ext .ts,.jsx,.js,.html,.vue . && stylelint \"**/*.{css,scss}\" && prettier \"**/*.{js,ts,jsx,vue,scss,css,json,html}\" --check",
        "precommit": "npm run lint",
        "pretty": "prettier --write \"**/*.{js,ts,jsx,vue,scss,css,json,html}\"",
        "generate": "nuxt generate",
        "prepare": "husky"
    },
    "dependencies": {
        "@nuxtjs/dotenv": "^1.4.2",
        "@pinia/nuxt": "^0.5.2",
        "bootstrap-icons": "^1.11.3",
        "dot-object": "^2.1.5",
        "js-cookie": "^3.0.5",
        "libphonenumber-js": "^1.11.4",
        "luxon": "^3.4.4",
        "nuxt": "^3.12.4",
        "nuxt-svgo": "^4.0.6",
        "pinia": "^2.2.0",
        "ts-node": "^10.9.2",
        "v3-infinite-loading": "^1.3.1",
        "vue-flatpickr-component": "^11.0.5",
        "vue3-recaptcha-v2": "^2.0.2",
        "vue3-virtual-scroller": "^0.2.3",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@bootstrap-vue-next/nuxt": "^0.26.22",
        "@nuxt/types": "^2.18.1",
        "@nuxt/typescript-build": "^3.0.2",
        "@nuxtjs/eslint-config-typescript": "^12.1.0",
        "@nuxtjs/eslint-module": "^3.1.0",
        "@nuxtjs/svg": "^0.4.1",
        "@types/lodash-es": "^4.17.12",
        "@types/luxon": "^3.4.2",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "bootstrap": "^5.3.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-nuxt": "^4.0.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^6.6.0",
        "eslint-plugin-vue": "^9.27.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.7",
        "lodash-es": "^4.17.21",
        "prettier": "^3.3.3",
        "rollup": "^2.79.1",
        "sass": "1.64.2",
        "sass-loader": "^10.5.2",
        "stylelint": "^15.11.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-config-standard-scss": "^11.1.0",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.29.0",
        "vite-svg-loader": "^5.1.0",
        "vue-tsc": "2.1.6"
    },
    "peerDependencies": {
        "@popperjs/core": "^2.11.8",
        "@rollup/plugin-json": "4.1.0",
        "@rollup/pluginutils": "3.1.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,html,vue}": "eslint --fix",
        "*.{css,scss}": "stylelint --fix",
        "*.{js,ts,jsx,vue,scss,css,json,html}": "prettier --write"
    },
    "pnpm": {
        "overrides": {
            "bootstrap-vue-next": "0.26.30",
            "stylelint": "15.11.0",
            "vue-tsc": "2.2.8"
        }
    }
